      "email": "jay@jayrylan.com"
    }
  ],
  "main": "./src/index",
  "keywords": [
    "jsonschema",
    "typescript"
  ],
  "scripts": {
    "api-check": "npm run build && api-extractor run",
    "build": "pack build",
    "test": "jest",
    "docs": "typedoc --theme markdown --out ./api-docs ./src"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/runtime-corejs2": "7.4.3",
    "@loomble/cspell-dictionary": "*",
    "@microsoft/api-extractor": "7.1.0",
    "@pika/pack": "0.3.7",
    "@pika/plugin-build-node": "0.3.15",
    "@pika/plugin-build-types": "0.3.16",
    "@pika/plugin-build-web": "0.4.0",
    "@pika/plugin-standard-pkg": "0.3.15",
    "@types/babel__core": "7.1.1",
    "@types/jest": "24.0.11",
    "@types/jest-diff": "20.0.1",
    "@types/node": "11.13.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.7.1",
    "babel-preset-slick": "7.0.4",
    "core-js": "3.0.1",
    "cspell": "3.2.8",
    "deep-sort-object": "1.0.2",
    "jest": "24.7.1",
    "jest-serializer-path": "0.1.15",
    "prettier": "1.17.0",
    "tsconfig-slick": "3.0.2",
    "tslint": "5.16.0",
    "tslint-slick": "5.0.0",
    "typedoc": "0.14.2",
    "typedoc-plugin-markdown": "1.2.0",
    "typescript": "3.4.5",
    "typescript-tslint-plugin": "0.3.1",
    "weak": "1.0.1"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "**/__*__/**/*",
            "**/website/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
